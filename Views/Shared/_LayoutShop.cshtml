<body class="header_sticky header-style-1 has-menu-extra">
    <!-- Preloader -->
    <div id="loading-overlay">
        <div class="loader"></div>
    </div>

    <!-- Boxed -->
    <div class="boxed">
        <div id="site-header-wrap">
            <!-- Header -->
            <header id="header" class="header header-container clearfix">
                <div class="container clearfix" id="site-header-inner">
                    <div id="logo" class="logo float-left">
                        <a href="Index" title="logo">
                            <img src="~/ShopLayoutAsset/images/logo.png" alt="image" width="107" height="24" data-retina="images/logo@2x.png" data-width="107" data-height="24">
                        </a>
                    </div><!-- /.logo -->
                    <div class="mobile-button"><span></span></div>
                    <ul class="menu-extra">
                        <li class="box-search">
                            <a class="icon_search header-search-icon" href="#"></a>
                            <form role="search" method="get" class="header-search-form" action="#">
                                <input type="text" value="" name="s" class="header-search-field" placeholder="Search...">
                                <button type="submit" class="header-search-submit" title="Search">Search</button>
                            </form>
                        </li>
                        <li class="box-login">
                            <a class="icon_login" href="#"></a>
                        </li>
                        <li class="box-cart nav-top-cart-wrapper">
                            <a class="icon_cart nav-cart-trigger active" href="#"><span>@Html.Action(actionName: "countCartItems", controllerName: "Shopping")</span></a>
                            <div class="nav-shop-cart">
                                <div class="widget_shopping_cart_content">
                                    <div class="woocommerce-min-cart-wrap">
                                        <ul class="woocommerce-mini-cart cart_list product_list_widget ">
                                            <li class="woocommerce-mini-cart-item mini_cart_item">
                                                <span>No Items in Shopping Cart</span>
                                            </li>
                                        </ul>
                                    </div><!-- /.widget_shopping_cart_content -->
                                </div>
                            </div><!-- /.nav-shop-cart -->
                        </li>
                    </ul><!-- /.menu-extra -->
                    <div class="nav-wrap">
                        @{ var department_Service = new BackEnd.Services.Department_Service();
                            var cart_Service = new BackEnd.Services.Cart_Service();
                            double total = 0; }
                        @using (Html.BeginForm("LogOff", "Account", FormMethod.Post, new { id = "logoutForm", @class = "navbar-right" }))
                        {
                            @Html.AntiForgeryToken()
                            <nav id="mainnav" class="mainnav">
                                <ul class="menu">
                                    <li class="active">
                                        <a href="Index">HOME</a>
                                    </li>
                                    <li>
                                        <a href="">SHOP</a>
                                        <ul class="submenu">
                                            <li>
                                                <a href="">Categories</a>
                                                <ul class="submenu">
                                                    @foreach (var department in department_Service.GetDepartments().OrderBy(x => x.Department_Name))
                                                    {
                                                        <li>
                                                            <a href="@Url.Action("Index","Shopping",new { id = department.Department_ID })">@department.Department_Name</a>
                                                        </li>
                                                    }
                                                </ul>
                                            </li>

                                        </ul>
                                    </li>
                                    @using Microsoft.AspNet.Identity
                                    @if (Request.IsAuthenticated)
                                    {
                                <li>
                                    @{ BackEnd.Models.ApplicationDbContext db = new BackEnd.Models.ApplicationDbContext(); }
                                    <a href="">Account</a>
                                    <ul class="submenu">
                                        <li>

                                            <a class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false"><span class="fa fa-user"></span>  @User.Identity.GetUserName() <span class="caret"></span></a>

                                        </li>
                                        <li><a href="javascript:document.getElementById('logoutForm').submit()"><i class="fa fa-lock"></i> Sign out</a></li>
                                        <li>@Html.ActionLink("Order History", "Order_History", "Orders")</li>
                                        <li>@Html.ActionLink("View Cart", "ShoppingCart", "Shopping")</li>
                                        <li>@Html.ActionLink("Order Tracking", "Customer_Orders", "Orders")</li>
                                    </ul>
                                </li>
                                    }


                                </ul>
                            </nav><!-- /.mainnav -->
                        }

                    </div><!-- /.nav-wrap -->
                </div><!-- /.container-fluid -->
            </header><!-- /header -->
        </div><!-- /#site-header-wrap -->
   
        <div class="container-fluid">
            @RenderBody()
        </div>



        <!-- Go Top -->
        <a class="go-top">
            <i class="fa fa-chevron-up"></i>
        </a>

    </div>

    <!-- Javascript -->
    <script src="~/ShopLayoutAsset/javascript/jquery.min.js"></script>
    <script src="~/ShopLayoutAsset/javascript/tether.min.js"></script>
    <script src="~/ShopLayoutAsset/javascript/bootstrap.min.js"></script>
    <script src="~/ShopLayoutAsset/javascript/jquery.easing.js"></script>
    <script src="~/ShopLayoutAsset/javascript/parallax.js"></script>
    <script src="~/ShopLayoutAsset/javascript/jquery-waypoints.js"></script>
    <script src="~/ShopLayoutAsset/javascript/jquery-countTo.js"></script>
    <script src="~/ShopLayoutAsset/javascript/jquery.countdown.js"></script>
    <script src="~/ShopLayoutAsset/javascript/jquery.flexslider-min.js"></script>
    <script src="~/ShopLayoutAsset/javascript/images-loaded.js"></script>
    <script src="~/ShopLayoutAsset/javascript/jquery.isotope.min.js"></script>
    <script src="~/ShopLayoutAsset/javascript/magnific.popup.min.js"></script>
    <script src="~/ShopLayoutAsset/javascript/jquery.hoverdir.js"></script>
    <script src="~/ShopLayoutAsset/javascript/owl.carousel.min.js"></script>
    <script src="~/ShopLayoutAsset/javascript/equalize.min.js"></script>
    <script src="~/ShopLayoutAsset/javascript/gmap3.min.js"></script>
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyAIEU6OT3xqCksCetQeNLIPps6-AYrhq-s&amp;region=GB"></script>
    <script src="~/ShopLayoutAsset/javascript/jquery-ui.js"></script>
    <script src="~/ShopLayoutAsset/javascript/switcher.js"></script>
    <script src="~/ShopLayoutAsset/javascript/jquery.cookie.js"></script>
    <script src="~/ShopLayoutAsset/javascript/main.js"></script>

    <!-- Revolution Slider -->
    <script src="~/ShopLayoutAsset/rev-slider/js/jquery.themepunch.tools.min.js"></script>
    <script src="~/ShopLayoutAsset/rev-slider/js/jquery.themepunch.revolution.min.js"></script>
    <script src="~/ShopLayoutAsset/javascript/rev-slider.js"></script>
    <!-- Load Extensions only on Local File Systems ! The following part can be removed on Server for On Demand Loading -->
    <script src="~/ShopLayoutAsset/rev-slider/js/extensions/revolution.extension.actions.min.js"></script>
    <script src="~/ShopLayoutAsset/rev-slider/js/extensions/revolution.extension.carousel.min.js"></script>
    <script src="~/ShopLayoutAsset/rev-slider/js/extensions/revolution.extension.kenburn.min.js"></script>
    <script src="~/ShopLayoutAsset/rev-slider/js/extensions/revolution.extension.layeranimation.min.js"></script>
    <script src="~/ShopLayoutAsset/rev-slider/js/extensions/revolution.extension.migration.min.js"></script>
    <script src="~/ShopLayoutAsset/rev-slider/js/extensions/revolution.extension.navigation.min.js"></script>
    <script src="~/ShopLayoutAsset/rev-slider/js/extensions/revolution.extension.parallax.min.js"></script>
    <script src="~/ShopLayoutAsset/rev-slider/js/extensions/revolution.extension.slideanims.min.js"></script>
    <script src="~/ShopLayoutAsset/rev-slider/js/extensions/revolution.extension.video.min.js"></script>


</body>

