        <th>
            @Html.DisplayNameFor(model => model.Storage)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NumTeachers)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NumStudents)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NumEmails)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PackageDescription)
        </th>
        <th>
